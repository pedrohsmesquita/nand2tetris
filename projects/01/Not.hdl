/**
 * Not gate:
 * out = not in
 */

/*
    A | out | Nand(A, A)
    0 |  1  |     1
    1 |  0  |     0
*\

CHIP Not {
    IN in;
    OUT out;

    PARTS:
        Nand(a=in, b=in, out=out);
}